    "@unicef-polymer/etools-upload": "^4.3.0",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "dayjs": "^1.10.4",
    "lit-element": "^2.4.0",
    "lit-translate": "^1.2.1",
    "lodash-es": "^4.17.21",
    "pwa-helpers": "^0.9.1",
